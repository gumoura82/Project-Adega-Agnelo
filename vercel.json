{
    "version": 2,
    "builds": [
      {
        "src": "src/pages/home/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/pages/home/index.html"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      }
    ]
  }